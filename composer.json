{
	"repositories": [
		{ "type": "path", "url": "packages/*" },
		{
			"type": "vcs",
			"url":  "git@gitlab.com:creationell_t3extensions/crea_theme_bs4.git"
		}
	],
	"name": "creationell/typo3-installer",
	"description" : "TYPO3 CMS Distribution with console and .env support",
	"license": "proprietary",
	"config": {
		"sort-packages": true,
		"platform": {
			"php": "7.4.9"
		},
		"allow-plugins": {
			"typo3/cms-composer-installers": true,
			"typo3/class-alias-loader": true,
			"helhum/dotenv-connector": true
		}
	},
	"require": {
		"amazing/media2click": "^1.3",
		"aoepeople/crawler": "^9.2",
		"b13/picture": "^1.0",
		"brotkrueml/form-country-select": "^1.3",
		"brotkrueml/schema": "^1.11",
		"christophlehmann/imageoptimizer": "^3.0",
		"clickstorm/cs-seo": "^6.3",
		"creationell/crea_theme_bs4": "dev-10lts",
		"creationell/crea_theme_bs4_child": "@dev",
		"dirkpersky/typo3-dp_cookieconsent": "^11.0",
		"fixpunkt/backendtools": "^2.1",
		"friendsoftypo3/frontend-editing": "^2.0",
		"georgringer/news": "^8.5",
		"gridelementsteam/gridelements": "^10.1",
		"helhum/typo3-config-handling": "^1.0",
		"helhum/typo3-console": "^6.3",
		"lavitto/typo3-form-to-database": "^2.0",
		"lochmueller/sourceopt": "^3.0",
		"lochmueller/staticfilecache": "^12.2",
		"mask/mask": "^6.6",
		"plan2net/webp": "^3.3",
		"sitegeist/fluid-components": "^2.5",
		"sitegeist/fluid-styleguide": "^1.9",
		"t3/fluid_page_cache": "^1.1",
		"t3brightside/advancedtitle": "^0.4",
		"teaminmedias-pluswerk/ke_search": "^3.8",
		"typo3-console/composer-auto-commands": "^0.4",
		"typo3/cms-about": "^10.4",
		"typo3/cms-adminpanel": "^10.4",
		"typo3/cms-belog": "^10.4",
		"typo3/cms-beuser": "^10.4",
		"typo3/cms-context-help": "^10.4",
		"typo3/cms-dashboard": "^10.4",
		"typo3/cms-felogin": "^10.4",
		"typo3/cms-filemetadata": "^10.4",
		"typo3/cms-fluid-styled-content": "^10.4",
		"typo3/cms-form": "^10.4",
		"typo3/cms-impexp": "^10.4",
		"typo3/cms-info": "^10.4",
		"typo3/cms-info-pagetsconfig": "^10.4",
		"typo3/cms-linkvalidator": "^10.4",
		"typo3/cms-lowlevel": "^10.4",
		"typo3/cms-opendocs": "^10.4",
		"typo3/cms-recycler": "^10.4",
		"typo3/cms-redirects": "^10.4",
		"typo3/cms-reports": "^10.4",
		"typo3/cms-rte-ckeditor": "^10.4",
		"typo3/cms-scheduler": "^10.4",
		"typo3/cms-seo": "^10.4",
		"typo3/cms-setup": "^10.4",
		"typo3/cms-sys-note": "^10.4",
		"typo3/cms-t3editor": "^10.4",
		"typo3/cms-tstemplate": "^10.4",
		"typo3/cms-viewpage": "^10.4",
		"typo3/minimal": "^10.4",
		"undefined/translate-locallang": "^2.8",
		"zwo3/mask_kesearch_indexer": "^1.6.4"
	},
	"require-dev": {
		"helhum/dotenv-connector": "^3.0",
		"typo3-console/composer-typo3-auto-install": "^0.4",
		"typo3-console/php-server-command": "^0.2"
	},
	"extra": {
		"typo3/cms": {
			"web-dir": "../../web"
		}
	},
	"suggest": {
		"t3g/blog":  "Flexible blog system for TYPO3",
		"extcode/cart": "Simple (yet extendable) shopping cart extension",
		"ichhabrecht/content-defender":  "Disallow certain content elements in columns of your BE layouts.",
		"in2code/fetchurl": "Fetch an url and show its content in the frontend.",
		"fixpunkt/fp-masterquiz":  "Quiz, polls and tests.",
		"clickstorm/go_maps_ext": "Google maps integration.",
		"derhansen/plain-faq":  "FAQ with list and detail view",
		"in2code/powermail": "Advanced forms.",
		"evoweb/recaptcha":  "Google Recaptcha integration for EXT:form",
